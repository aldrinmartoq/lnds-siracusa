aldrin-macbookpro:lnds-siracusa amartoq$ make clean && make
rm -f siracusa-performante siracusa-optm-1 siracusa-optm-2 siracusa-optm-3 siracusa-optm-4 siracusa-lnds-1 siracusa-lnds-2  *.txt
clang -O4    siracusa-performante.c   -o siracusa-performante
clang -O4    siracusa-optm-1.c   -o siracusa-optm-1
clang -O4    siracusa-optm-2.c   -o siracusa-optm-2
clang -O4    siracusa-optm-3.c   -o siracusa-optm-3
clang -O4    siracusa-optm-4.c   -o siracusa-optm-4
clang -O4    siracusa-lnds-1.c   -o siracusa-lnds-1
clang -O4    siracusa-lnds-2.c   -o siracusa-lnds-2
for n in 1000000 5000000 10000000 50000000 100000000 500000000 1000000000 ; do \
		echo "n = $n"; \
		cat /dev/null > run-$n.txt; \
		for prog in siracusa-performante siracusa-optm-1 siracusa-optm-2 siracusa-optm-3 siracusa-optm-4 siracusa-lnds-1 siracusa-lnds-2 ; do \
			printf "%20s %10s " $prog $n >> run-$n.txt; \
			/usr/bin/time -p ./$prog $n > tmp.txt 2>&1; \
			printf "%10s %s %s " `head -1 tmp.txt` clang -O4 >> run-$n.txt; \
			grep real tmp.txt | awk '{print $2}' >> run-$n.txt; \
			tail -1 run-$n.txt; \
			done; \
		echo; \
	done
